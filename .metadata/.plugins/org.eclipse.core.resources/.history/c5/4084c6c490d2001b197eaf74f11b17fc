
public class CheckWhetherAlphabetOrNot {
	
	public static void main(String[] args)
	{
		String c = "a$999";
		if(c.equalsIgnoreCase(c.replaceAll("[^a-zA-Z]", "")))
		{
			System.out.println("Given String is alphabet : "+c);
		}
		else
		{
			System.out.println("Given String is not alphabaet : "+c);
		}
		
		char a = '$';
		if((a >= 'a' && a<= 'z') || (a>='A' && a<= 'Z'))
		{
			System.out.println("Given Character is alphabet : "+a);
		}
		else
		{
			System.out.println("Given Character is not alphabet : "+a);
		}
		
		
	}

}
